<html>
<head>
</head>
<body>
<form class="aui" method="POST" action="/jira/rest/cab/1.0/systems/post" enctype="application/json">
    <div class="field-group">
        <label for="system">Система</label>
        <select class="select" id="system" name="system">
            <option value="JDA - планограммы ТСХ">JDA - планограммы ТСХ</option>
        </select>
    </div>
    <div class="field-group">
        <label for="typechange">Тип измения</label>
        <select class="select" id="typechange" name="typechange">
            <option>Select</option>
            <option value="Bugfix">Bugfix</option>
            <option value="Активация настроек">Активация настроек</option>
            <option value="Внерелиз">Внерелиз</option>
            <option value="Дефект">Дефект</option>
            <option value="Внерелиз">Внерелиз</option>
            <option value="Изменение настроек">Изменение настроек</option>
            <option value="Массовая выгрузка">Массовая выгрузка</option>
            <option value="Предсогласованное изменение">Предсогласованное изменение</option>
            <option value="Релиз">Релиз</option>
            <option value="Спринт">Спринт</option>
            <option value="Фоновое задание">Фоновое задание</option>
        </select>
    </div>
    <div class="field-group">
        <label for="step1">Стадия 1</label>
        <input class="text medium-field" type="text" id="step1" name="step1" value="$!{step1}">
    </div>
    <div class="field-group">
        <label for="step2">Стадия 21</label>
        <input class="text medium-field" type="text" id="step21" name="step21" value="$!{step21}">
    </div>
    <div class="field-group">
        <label for="step2">Стадия 22</label>
        <input class="text medium-field" type="text" id="step22" name="step22" value="$!{step22}">
    </div>
    <div class="field-group">
        <label for="step2">Стадия 23</label>
        <input class="text medium-field" type="text" id="step23" name="step23" value="$!{step23}">
    </div>
    <div class="field-group">
        <label for="step3">Стадия 3</label>
        <input class="text medium-field" type="text" id="step3" name="step3" value="$!{step3}">
    </div>
    <div class="field-group">
        <label for="autorize">Авторизующий</label>
        <input class="text medium-field" type="text" id="autorize" name="autorize" value="$!{autorize}">
    </div>
    <div class="field-group">
        <label for="active">Система активна?</label>
        <select class="select" id="active" name="active">
            <option>Select</option>
            <option value="true">Да</option>
            <option value="false">Нет</option>
        </select>
    </div>
    <div class="buttons-container">
        <div class="buttons">
            <input class="button submit" type="submit" value="Save" id="comment-save-button">
            <a class="cancel" href="#">Cancel</a>
        </div>
    </div>
</form>
    $webResourceManager.requireResource("ru.pestov.alexey.plugins.spring.issue-assigneer:selects-resources")
</body>
</html>