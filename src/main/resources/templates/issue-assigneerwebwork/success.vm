<html>
<head>
</head>
<body>
<form class="aui" method="POST" action="IssueAssigneerWebworkAction!save.jspa" enctype="application/json">
    <div class="field-group">
        <label for="systemCab">Система</label>
        <select class="select" id="systemCab" name="systemCab">
            <option>Select</option>
        </select>
    </div>
    <div class="field-group">
        <label for="typechange">Тип измения</label>
        <select class="select" id="typechange" name="typechange">
            <option>Select</option>
            <option value="1">Bugfix</option>
            <option value="2">Активация настроек</option>
            <option value="3">Внерелиз</option>
            <option value="4">Дефект</option>
            <option value="5">Изменение настроек</option>
            <option value="6">Массовая выгрузка</option>
            <option value="7">Предсогласованное изменение</option>
            <option value="8">Релиз</option>
            <option value="9">Спринт</option>
            <option value="10">Фоновое задание</option>
        </select>
    </div>
    <div class="field-group">
        <label for="stage1">Стадия 1</label>
        <input class="text medium-field" type="text" id="stage1" name="stage1" value="$!{stage1}" disabled>
    </div>
    <div class="field-group">
        <label for="stage21">Стадия 21</label>
        <input class="text medium-field" type="text" id="stage21" name="stage21" value="$!{stage21}" disabled>
    </div>
    <div class="field-group">
        <label for="stage2">Стадия 22</label>
        <input class="text medium-field" type="text" id="stage22" name="stage22" value="$!{stage22}" disabled>
    </div>
    <div class="field-group">
        <label for="stage2">Стадия 23</label>
        <input class="text medium-field" type="text" id="stage23" name="stage23" value="$!{stage23}" disabled>
    </div>
    <div class="field-group">
        <label for="stage3">Стадия 3</label>
        <input class="text medium-field" type="text" id="stage3" name="stage3" value="$!{stage3}" disabled>
    </div>
    <div class="field-group">
        <label for="autorize">Авторизующий</label>
        <input class="text medium-field" type="text" id="autorize" name="autorize" value="$!{autorize}" disabled>
    </div>
    <div class="field-group">
        <label for="active">Система активна?</label>
        <select class="select" id="active" name="active" disabled>
            <option>Select</option>
            <option value="true">Да</option>
            <option value="false">Нет</option>
        </select>
    </div>
    <div class="buttons-container">
        <div class="buttons">
            <input class="button submit" type="submit" value="Save" id="comment-save-button">
            <a class="cancel" href="#">Cancel</a>
        </div>
    </div>
</form>
    $webResourceManager.requireResource("ru.pestov.alexey.plugins.spring.issue-assigneer:selects-resources")
</body>
</html>